# DevAI Environment Configuration
# Copy this file to .env and fill in your values

# Context store location (default: ./context_store)
# DEVAI_STORE_PATH=./context_store

# Default working directory (default: current directory)
# DEVAI_WORKING_DIR=.

# Enable audit logging (default: true)
# DEVAI_AUDIT_ENABLED=true

# Log level (default: INFO)
# DEVAI_LOG_LEVEL=INFO
